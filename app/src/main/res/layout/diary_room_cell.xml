<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".adapter.DiaryRoomAdapter">

    <TextView
        android:id="@+id/diaryRoomDate"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="yyyy-mm-dd"
        app:layout_constraintBottom_toTopOf="@+id/diaryRoomContent"
        app:layout_constraintEnd_toEndOf="@+id/diaryRoomContent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/diaryRoomContent" />

    <TextView
        android:id="@+id/diaryRoomContent"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="일기 내용ㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁㅁ"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="29dp" />

</androidx.constraintlayout.widget.ConstraintLayout>